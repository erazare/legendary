{
	// Enables Fog that forms when it rains
	"Client_PrecipitationFog": false,
	// Enables new particle-based weather effects (This feature hasn't been implemented yet!)
	"Client_ParticlePrecipitation": false,
	// Makes particles move with the wind if enabled
	"Client_ParticleWind": true,
	// Makes non-storm clouds translucent when enabled
	"Client_TranslucentClouds": true,
	// Changes the render distance of the cloud particles. Only adjust this if your computer can't handle the particles
	"Client_CloudParticleRenderDistance": 512.0,
	// The minimum wind speed, can be a number from 0-100
	"Wind_MinimumSpeed": 0,
	// The maximum wind speed, can be a number from 0-100
	"Wind_MaximumSpeed": 100,
	// The chance of a low wind event occurring
	"Wind_LowWindStartChance": 4000,
	// The chance of a high wind event occurring
	"Wind_HighWindStartChance": 8000,
	// The minimum duration (in ticks) of a low wind event
	"Wind_LowWindDurationBase": 2400,
	// The minimum duration (in ticks) of a high wind event
	"Wind_HighWindDurationBase": 2400,
	// The amount of maximum extra ticks that can be added to the base duration of a low wind event
	"Wind_LowWindDurationExtra": 12000,
	// The amount of maximum extra ticks that can be added to the base duration of a high wind event
	"Wind_HighWindDurationExtra": 12000,
	// If enabled, a random weather condition will occur in a new world
	"Weather_RandomConditionInNewWorld": true,
	// If enabled, supercells can form tornadoes
	"Weather_TornadoesCanSpawn": true,
	// Changes the chance of a lightning strike occurring
	"Weather_LightningStrikeBaseChance": 25000,
	// Adjusts the minimum amount of water a cloud needs to collect for it to start precipitating
	"Weather_MinimumWaterToPrecipitate": 100,
	// The chance of a cloud collecting water from nothing
	"Weather_WaterCollectionFromNothingChance": 100,
	// The chance of a cloud collecting water from a humid biome
	"Weather_WaterCollectionFromBiomeChance": 15,
	// The chance of a precipitation capable cloud precipitating
	"Weather_PrecipitationChance": 50,
	// The chance of a isolated supercell forming (This chance is rolled daily)
	"Weather_SupercellChance": 15,
	// The chance of a squall line forming (This chance is rolled daily)
	"Weather_SquallLineChance": 15
}